{
  "schemaVersion": 1,
  "id": "culinaryquest",
  "version": "${version}",

  "name": "Culinary Quest",
  "description": "A mod that adds 750+ food items to minecraft from Morocco to Japan.",
  "authors": [],
  "contact": {},

  "license": "MIT",
  "icon": "assets/culinaryquest/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": ["net.lexicodes.culinaryquest.client.CulinaryquestClient"],
    "main": ["net.lexicodes.culinaryquest.Culinaryquest"]
  },

  "mixins": [
    "culinaryquest.mixins.json"
    ,{
      "config": "culinaryquest.client.mixins.json",
      "environment": "client"
    }
  ],

  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
